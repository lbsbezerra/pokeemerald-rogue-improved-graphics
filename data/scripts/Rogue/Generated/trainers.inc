gTrainerEncounterText_4::
# 26 "data/scripts/Rogue/Generated/trainers.pory"
	.string "You're better than I though.\p"
	.string "Go on ahead.$"

gTrainerEncounterText_5::
# 31 "data/scripts/Rogue/Generated/trainers.pory"
	.string "…Things shouldn't be this way!$"

gTrainerEncounterText_6::
# 36 "data/scripts/Rogue/Generated/trainers.pory"
