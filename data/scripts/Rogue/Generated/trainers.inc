gTrainerEncounterText_9::
# 51 "data/scripts/Rogue/Generated/trainers.pory"
	.string "How could I lose?$"

gTrainerEncounterText_10::
# 56 "data/scripts/Rogue/Generated/trainers.pory"
	.string "{STR_VAR_1}: Through rigorous\n"
	.string "training, people and Pokémon can\l"
	.string "become stronger without limit.$"

gTrainerEncounterText_11::
# 61 "data/scripts/Rogue/Generated/trainers.pory"
	.string "My Pokémon will grind you down with our\n"
	.string "superior power!$"

gTrainerEncounterText_12::
# 66 "data/scripts/Rogue/Generated/trainers.pory"
	.string "I have nothing else to say.\p"
	.string "Run along now, child!$"

gTrainerEncounterText_13::
# 71 "data/scripts/Rogue/Generated/trainers.pory"
	.string "Oh, my!\n"
	.string "You're something special, child$"

gTrainerEncounterText_14::
# 76 "data/scripts/Rogue/Generated/trainers.pory"
