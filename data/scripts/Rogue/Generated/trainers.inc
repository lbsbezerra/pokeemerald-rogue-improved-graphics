	.string "battling!$"

gTrainerEncounterText_15::
# 81 "data/scripts/Rogue/Generated/trainers.pory"
	.string "I'll show you how a real Trainer battles!$"

gTrainerEncounterText_16::
# 86 "data/scripts/Rogue/Generated/trainers.pory"
